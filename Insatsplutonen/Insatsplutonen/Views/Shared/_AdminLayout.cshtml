<!DOCTYPE html>
<html lang="en">
<head>
    <title>@ViewBag.Title</title>
    <meta charset="utf-8">
    <!--[if IE]><meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"><![endif]-->

    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="Insatsplutonen" />
    <meta name="author" content="Haris K" />

    <link rel="stylesheet" href="~/Content/admin/assets/js/jquery-ui/css/no-theme/jquery-ui-1.10.3.custom.min.css">
    <link rel="stylesheet" href="~/Content/admin/assets/css/font-icons/entypo/css/entypo.css">
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Noto+Sans:400,700,400italic">
    <link rel="stylesheet" href="~/Content/admin/assets/css/neon.css">
    <link rel="stylesheet" href="~/Content/admin/assets/css/custom.css">
    <link rel="stylesheet" href="~/Content/admin/assets/js/select2/select2-bootstrap.css">
    <link rel="stylesheet" href="~/Content/admin/assets/js/select2/select2.css">
    <link rel="stylesheet" href="~/Content/admin/assets/js/selectboxit/jquery.selectBoxIt.css">
    <link rel="stylesheet" href="~/Content/admin/assets/js/daterangepicker/daterangepicker-bs3.css">
    <link rel="stylesheet" href="~/Content/admin/assets/js/wysihtml5/bootstrap-wysihtml5.css">
    @Styles.Render("//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body class="page-body" data-url="http://neon.dev">

    <div class="page-container">

        @Html.Partial("~/Views/Admin/_AdminMenu.cshtml")

        <div class="main-content">
            @Html.Partial("~/Views/Admin/_Userinfo.cshtml")
            @RenderBody()

        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
    <script src="~/Content/admin/assets/js/gsap/main-gsap.js"></script>
    <script src="~/Content/admin/assets/js/jquery-ui/js/jquery-ui-1.10.3.minimal.min.js"></script>
    <script src="~/Content/admin/assets/js/joinable.js"></script>
    <script src="~/Content/admin/assets/js/resizeable.js"></script>
    <script src="~/Content/admin/assets/js/neon-api.js"></script>
    <script src="~/Content/admin/assets/js/jquery.validate.min.js"></script>
    <script src="~/Content/admin/assets/js/neon-custom.js"></script>
    <script src="~/Content/admin/assets/js/neon-demo.js"></script>

    <script src="~/Content/admin/assets/js/select2/select2.min.js"></script>
    <script src="~/Content/admin/assets/js/bootstrap-tagsinput.min.js"></script>
    <script src="~/Content/admin/assets/js/typeahead.min.js"></script>
    <script src="~/Content/admin/assets/js/selectboxit/jquery.selectBoxIt.min.js"></script>
    <script src="~/Content/admin/assets/js/bootstrap-datepicker.js"></script>
    <script src="~/Content/admin/assets/js/bootstrap-timepicker.min.js"></script>
    <script src="~/Content/admin/assets/js/bootstrap-colorpicker.min.js"></script>
    <script src="~/Content/admin/assets/js/daterangepicker/moment.min.js"></script>
    <script src="~/Content/admin/assets/js/daterangepicker/daterangepicker.js"></script>
    <script src="~/Content/admin/assets/js/jquery.multi-select.js"></script>
</body>
</html>
